<h1>Edit  Page</h1>

<%= form_for(@page, :method => 'delete') do |f| %>
<table>
<tr>
    <td>Position :<%= f.number_field(:position) %>
</td>
</tr>
    <tr>
        <td>Name :<%= f.text_field(:name) %>
    </td>
    </tr>
    <tr>
        <td>Permalink :<%= f.text_field(:permalink) %>
    </td>
    </tr>
    <tr>
        <td>Visible :<%= f.text_field(:visible) %>
    </td>
    </tr>
    <tr>
        <td>Subject ID :<%= f.text_field(:subject_id) %>
    </td>
    </tr>
    <tr>
        <td><%= submit_tag("Delete")%>
    </td>
    </tr>
</table>
<% end %>
 